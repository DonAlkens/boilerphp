{
    "name": "Boiler Framework",
    "description": "",
    "version": "1.0.0",
    "mode": "development",
    "databaseConnections": {
        "default": {
            "driver": "${DB_DRIVER}",
            "host": "${DB_HOST}",
            "username": "${DB_USERNAME}",
            "password": "${DB_PASSWORD}",
            "database": "${DB_NAME}",
            "port": "${DB_PORT}"
        }
    },
    "mailAttributes": {
        "driver": "phpmailer",
        "smtp": {
            "smtpHost": "${SMTP_HOST}",
            "smtpUsername": "${SMTP_USERNAME}",
            "smtpPassword": "${SMTP_PASSWORD}",
            "smtpPort": "${SMTP_PORT}",
            "smtpEncryption": "${SMTP_ENCRYPTION}"
        }
    }
}